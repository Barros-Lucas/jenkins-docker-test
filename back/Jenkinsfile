pipeline {
  agent any
  tools {
    maven 'M3'
  }
  stages {
    stage('Build image') {
      steps {
        	sh  'cd back/'
	      	sh 'ls'
	      	sh 'mvn package'
		sh 'mkdir -p target/dependency && (cd target/dependency; jar -xf ../*jar)'
		
		dockerfile {
			filename 'dashboard/back-test-jenkins'
			dir 'back'
		}
      }
    }
  }
}
